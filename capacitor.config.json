{
    "appId": "com.sotacib.commercial",
    "appName": "SOTACIB Commercial",
    "webDir": "out",
    "server": {
        "url": "http://localhost:3000",
        "cleartext": true,
        "androidScheme": "http",
        "iosScheme": "http"
    },
    "ios": {
        "contentInset": "always",
        "allowsLinkPreview": false,
        "webViewConfiguration": {
            "allowsBackForwardNavigationGestures": true,
            "allowsInlineMediaPlayback": true
        }
    },
    "plugins": {
        "CapacitorHttp": {
            "enabled": true
        }
    }
}